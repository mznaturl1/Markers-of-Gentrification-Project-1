   "source": [
    "# Read CSV\n",
    "csvpath = \"output_data/census.csv\"\n",
    "census_df = pd.read_csv(csvpath,index_col=\"datapoint\")\n",
    "census_df[\"Zipcode\"] = census_df[\"Zipcode\"].astype(\"category\")\n",
    "census_df"
   ]
